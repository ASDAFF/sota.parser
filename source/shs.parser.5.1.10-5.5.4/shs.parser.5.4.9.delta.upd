    protected function convetCyrillic(&$url)
    {
        if(preg_match("/^\/{2}www/", $url))
        {
            $url = preg_replace("/^\/{2}www/", "www", $url);
        }
        $converter = new sotbit_idna_convert();
        $url = $converter->encode($url);
    }
    
