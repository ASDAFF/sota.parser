            $url = "http://".$url;
        }elseif(preg_match("/https:/", $this->rss))
        {
            $url = str_replace("https://", "", $this->rss);
            $url = preg_replace("/\/.*/", "", $url);
            $url = "https://".$url;    
        }
        else{
            $url = preg_replace("/\/.*/", "", $this->rss);
        }
        return $url;
    }

