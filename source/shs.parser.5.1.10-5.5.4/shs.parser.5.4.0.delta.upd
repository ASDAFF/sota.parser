            $price = trim(strip_tags($price));
            $this->arPriceOffer["PRICE"] = $price;
            $this->arPriceOffer["CATALOG_GROUP_ID"] = $this->settings["catalog"]["price_type"];
            $this->arPriceOffer["CURRENCY"] = $this->settings["catalog"]["currency"];
