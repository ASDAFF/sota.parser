    protected function parseSelectorStyle($attr, $src)
    {
        if($attr=="style" && $src)
        {
            preg_match("/url\(([^)]*)\)/", $src, $matches);
            if(isset($matches[1]) && $matches[1])
            {
                $src = str_replace(array('"', "'"), "", $matches[1]);
            }
        }

        return $src;
    }

